    <% include partials/report-header %>

      <main role="main">

        <div class="jumbotron">
          <div class="container">
            <h2><%= title %></h2>
          </div>
        </div>

        <div class="container">

          <div class="row">
            <div class="col-md-12">

              <h4><%= locationName %></h4>
              <h6><%= locationAddress %></h6>
              <h6><%= locationCity %> <%= locationState %>, <%= locationZip %></h6>

              <canvas id="myChart" width="100%" height="100%"></canvas>
              <script>

              var ctx = document.getElementById("myChart");
              var myChart = new Chart(ctx, {
                  type: 'line',
                  data: {
                      labels: <%- JSON.stringify(allObligationEndDates); %>,
                      datasets: [{
                          label: 'Return Total',
                          data: <%- JSON.stringify(allReturnTotals); %>,
                          backgroundColor: [
                              'rgba(255, 99, 132, 0.2)',
                          ],
                          borderColor: [
                              'rgba(255, 99, 132, 1)',
                          ],
                          borderWidth: 1,
                          fill: true,
                          lineTension: 0
                      },
                      {
                          label: 'Return Total 2',
                          data: <%- JSON.stringify(allReturnTotalsSecondary); %>,
                          backgroundColor: [
                              'rgba(0, 255, 0, 0.2)',
                          ],
                          borderColor: [
                              'rgba(0, 255, 0, 1)',
                          ],
                          borderWidth: 1,
                          fill: true,
                          lineTension: 0
                      }
                    ]
                  },
                  options: {
                      scales: {
                          yAxes: [{
                              ticks: {
                                  beginAtZero:true
                              }
                          }]
                      }
                  }
              });



              </script>

            </div>
          </div>

          <hr>

        </div>

      </main>

      <% include partials/footer %>
