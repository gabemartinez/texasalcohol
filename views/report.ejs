    <% include partials/report-header %>

      <main role="main">

        <div class="jumbotron">
          <div class="container">
            <h2><%= title %></h2>
          </div>
        </div>

        <div class="container">

          <div class="row">

            <div class="col-md-12">

              <canvas id="myChart" width="100%" height="100%"></canvas>
              <script>
              var ctx = document.getElementById("myChart");
              var myChart = new Chart(ctx, {
                  type: 'bar',
                  data: {
                      labels: <%- JSON.stringify(allObligationEndDates); %>,
                      datasets: [{
                          label: 'Return Total',
                          data: <%- JSON.stringify(allReturnTotals); %>,
                          backgroundColor: [
                              'rgba(255, 99, 132, 0.2)',
                              'rgba(54, 162, 235, 0.2)',
                              'rgba(255, 206, 86, 0.2)',
                              'rgba(75, 192, 192, 0.2)',
                              'rgba(153, 102, 255, 0.2)',
                              'rgba(255, 159, 64, 0.2)'
                          ],
                          borderColor: [
                              'rgba(255,99,132,1)',
                              'rgba(54, 162, 235, 1)',
                              'rgba(255, 206, 86, 1)',
                              'rgba(75, 192, 192, 1)',
                              'rgba(153, 102, 255, 1)',
                              'rgba(255, 159, 64, 1)'
                          ],
                          borderWidth: 1
                      }]
                  },
                  options: {
                      scales: {
                          yAxes: [{
                              ticks: {
                                  beginAtZero:true
                              }
                          }]
                      }
                  }
              });
              </script>

            </div>

          </div>

          <hr>

        </div>

      </main>

      <% include partials/footer %>
