  <% include partials/header %>

    <main role="main">

      <div class="jumbotron">
        <div class="container">
          <h2>Texas MixedBev Analytics</h2>
          <!-- <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more &raquo;</a></p> -->
        </div>
      </div>

      <div class="container">

        <div class="row">

          <div class="col-md-9">
            <h1><%= locationName %></h1>
            <p>Taxpayer #: <%= taxpayerNumber %></p>
            <p>TABC Permit #: <%= tabcPermitNumber %></p>
            <p><%= locationAddress %></p>
            <p><%= locationCity %>, <%= locationState %> <%= locationZip %></p>
            <p>
              <%if (locationPhone == '(0NN) NNN-NNNN') { %>
                NA
              <% } else { %>
                <%= locationPhone %>
              <% } %>
            </p>
          </div>

          <div class="col-md-3">

            <table class="table text-center">
              <thead>
                <p class="text-center font-italic">Rank Based on Revenue</p>
                <tr>
                  <th scope="col">Zip</th>
                  <th scope="col">County</th>
                  <th scope="col">Texas</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td>88</td>
                  <td>88</td>
                  <td>88</td>
                </tr>
              </tbody>
            </table>

          </div>

        </div>

        <div class="row">

          <div class="col-md-3">
            <h2>Product Mix</h2>
          </div>

          <div class="col-md-7">
            <h2>Line Graph</h2>
          </div>

          <div class="col-md-2">
            <h2>Add Bars to Compare</h2>
          </div>

        </div>

        <div class="row">

          <div class="col-md-4">
            <h2>Monthly Revenue</h2>
            <p>Last 12 Months Listed</p>
          </div>

          <div class="col-md-8">
            <h2>Performance</h2>
            <p>Indicators</p>
            <p>Name</p>
            <p>Similar Bars</p>
          </div>

        </div>

        <div class="row">

          <div class="col-md-12">
            <h2>Similar Bars</h2>
          </div>

        </div>

        <div class="row">

          <div class="col-md-6">
            <h5>Totals</h5>
            <p>Total: <%= returnTotal %></p>
            <p>Liquor: <%= liquorReceipts %></p>
            <p>Wine: <%= wineReceipts %></p>
            <p>Beer: <%= beerReceipts %></p>
          </div>

          <div class="col-md-6">
            <h5>Taxpayer Info</h5>
            <p><%= taxpayerName %></p>
            <p><%= taxpayerAddress %></p>
            <p><%= taxpayerCity %>, <%= taxpayerState %> <%= taxpayerZip %></p>
            <p>
              <%if (taxpayerPhone == '(0NN) NNN-NNNN') { %>
                NA
              <% } else { %>
                <%= taxpayerPhone %>
              <% } %>
            </p>
          </div>

        </div>

        <hr>

      </div>

    </main>

    <% include partials/footer %>
